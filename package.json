    "test": "vitest run",
    "lint:fix": "eslint \"./**/*.{js,mjs,cjs,ts,jsx,tsx}\" --fix",
    "format": "prettier --check \"./**/*.{js,jsx,ts,tsx,json,yaml,yml,md}\"",
