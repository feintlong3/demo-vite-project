    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1.0.1"
  },
  "volta": {
    "node": "22.12.0",
    "pnpm": "10.12.3"
  }
